    "null-loader": "^0.1.1",
    "nyc": "^5.6.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.7",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-select": "^1.0.0-beta9",
    "serve-favicon": "^2.3.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
