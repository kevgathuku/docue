    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flux": "^2.1.1",
    "http-proxy": "^1.13.1",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "5.5.4",
