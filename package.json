{
  "name": "react-docms",
  "version": "0.1.1",
  "engines": {
    "node": "5.4.1",
    "npm": "3.5.3"
  },
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "test:fend": "jest",
    "test:bend": "jasmine",
    "test": "NODE_ENV='test' npm run test:bend && npm run test:fend",
    "coverage": "NODE_ENV='test' nyc --require babel-core/register npm t && nyc report --reporter=text-lcov | coveralls",
    "deploy": "webpack -p --config webpack.production.config.js --progress --colors",
    "postinstall": "if [ -n \"$npm_config_production\" ]; then ./node_modules/bower/bin/bower install && npm run deploy; fi"
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flux": "^2.1.1",
    "http-proxy": "^1.13.1",
    "jasmine": "^2.4.1",
    "jsonwebtoken": "5.5.4",
    "keymirror": "^0.1.1",
    "moment": "^2.11.2",
    "mongoose": "^4.4.3",
    "morgan": "~1.7.0",
    "nodemon": "^1.8.1",
    "nyc": "^5.6.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.7",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-select": "^1.0.0-beta9",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
    "superagent": "^1.7.2",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^6.0.1",
    "coveralls": "^2.11.6",
    "dotenv": "^2.0.0",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.2.0",
    "react-addons-test-utils": "^0.14.7",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/utils/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "app/components/Landing/index.jsx": true
    }
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
