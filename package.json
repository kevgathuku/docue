{
  "name": "react-docms",
  "version": "0.1.1",
  "engines": {
    "node": "5.4.1",
    "npm": "3.5.3"
  },
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "test:fend": "./node_modules/.bin/karma start",
    "test:bend": "jasmine",
    "test": "NODE_ENV='test' npm run coverage && npm run test:fend",
    "coverage": "NODE_ENV='test' nyc --require babel-core/register npm run test:bend && nyc report --reporter=text-lcov | coveralls",
    "deploy": "webpack -p --config webpack.production.config.js --progress --colors",
    "postinstall": "if [ -n \"$npm_config_production\" ]; then ./node_modules/bower/bin/bower install && npm run deploy; fi"
  },
  "dependencies": {
    "async": "^1.5.2",
