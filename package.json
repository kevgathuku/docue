{
  "name": "react-docms",
  "version": "0.1.1",
  "engines": {
    "node": "6.2.1",
    "npm": "3.9.3"
  },
  "private": true,
  "scripts": {
    "prestart": "./node_modules/.bin/migrate",
    "dev-server": "babel-node index.js",
    "start": "nodemon index.js",
    "test:fend": "./node_modules/.bin/karma start",
    "test:bend": "NODE_ENV='test' babel-node spec/run.js",
    "test": "NODE_ENV='test' npm run coverage && npm run test:fend",
    "coverage": "nyc --require babel-core/register npm run test:bend && nyc report --reporter=text-lcov | coveralls",
    "codecov": "cat coverage/lcov/lcov.info | codecov",
    "deploy": "rimraf ./public/build && NODE_ENV=production webpack -p --config webpack.production.config.js --progress --profile --colors",
    "postinstall": "if [ -n \"$npm_config_production\" ]; then ./node_modules/bower/bin/bower install && npm run deploy; fi"
  },
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "estraverse-fb": "^1.3.1",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flux": "^2.1.1",
    "http-proxy": "^1.13.1",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "7.0.1",
    "keymirror": "^0.1.1",
    "migrate": "^0.2.2",
    "moment": "^2.11.2",
    "mongoose": "^4.4.3",
    "morgan": "~1.7.0",
    "nodemon": "^1.8.1",
    "nyc": "^6.4.0",
    "object-assign": "^4.1.0",
    "react": "^15.0.1",
    "react-addons-update": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.0",
    "react-select": "^1.0.0-beta13",
    "rimraf": "^2.5.3",
    "serve-favicon": "^2.3.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "superagent": "^2.0.0",
    "supertest": "^1.2.0",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-airbnb": "^2.0.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "codecov": "^1.0.1",
    "concurrently": "^2.0.0",
    "coveralls": "^2.11.6",
    "dotenv": "^2.0.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "expect": "^1.14.0",
    "expect-jsx": "^2.6.0",
    "flow-bin": "^0.27.0",
    "ignore-styles": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jsdom": "^9.2.1",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-cli": "1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^15.1.0",
    "sinon": "^1.17.3",
    "sinon-spy-react": "^2.0.0",
    "supertest-as-promised": "^3.1.0",
    "webpack-dev-server": "1.14.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
